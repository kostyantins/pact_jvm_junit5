{
  "consumer": {
    "name": "aqa"
  },
  "interactions": [
    {
      "description": "Starships test interaction",
      "providerStates": [
        {
          "name": "Test GET /api/planets/3"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/api/planets/3"
      },
      "response": {
        "body": {
          "climate": "temperate, tropical",
          "diameter": "150000000",
          "gravity": "1 standard",
          "name": "Yavin IV",
          "orbital_period": "4818",
          "population": "1000",
          "residents": [
                        [
              "string",
              "string",
              "string"
            ]
          ],
          "rotation_period": "24",
          "surface_water": "8",
          "terrain": "jungle, rainforests"
        },
        "generators": {
          "body": {
            "$.residents[0][0]": {
              "size": 20,
              "type": "RandomString"
            },
            "$.residents[0][1]": {
              "size": 20,
              "type": "RandomString"
            },
            "$.residents[0][2]": {
              "size": 20,
              "type": "RandomString"
            }
          }
        },
        "headers": {
          "Content-Type": "application/json; charset=UTF-8"
        },
        "matchingRules": {
          "body": {
            "$.residents[0][0]": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.residents[0][1]": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.residents[0][2]": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {
            "Content-Type": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-jvm": {
      "version": "4.2.2"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
